    "seed": "ts-node src/scripts/seed.ts",
    "seed:full": "ts-node src/scripts/seed-complete.ts",
    "setup": "npm run init-db && npm run seed:full",
    "setup-couple": "ts-node src/scripts/setup-test-couple.ts",
    "check-users": "ts-node src/scripts/check-users.ts",
    "check-tables": "ts-node src/scripts/check-tables.ts",
    "check-db": "ts-node src/scripts/check-current-db.ts",
    "check-period": "ts-node src/scripts/check-period-table.ts",
    "init-pet": "ts-node src/scripts/init-pet-system.ts",
    "test": "echo \"测试功能待添加\" && exit 0"
  },
  "keywords": [
    "love",
    "couple",
    "mysql",
    "socketio",
    "typescript"
